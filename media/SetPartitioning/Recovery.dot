graph {
        bgcolor="transparent";
        node [fontname="Helvetica"];
        layout=fdp;

        A [label="{1}"];
        B [label="{2}"];
        C [label="{3}"];
        D [label="{4, 5}"];
        E [label="???"];        

        A -- B -- C -- D;
        A -- C;
        A -- D;
        B -- D;             

        {
                edge [color=red, style=dashed];

                A -- E;
                B -- E;
                C -- E;
                D -- E;                
        }
}
