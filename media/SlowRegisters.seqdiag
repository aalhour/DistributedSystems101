seqdiag {
        activation = none;
        default_fontsize = 14;
      
        A [label="Process A"];
        R [label="Register"];
        
        A ->> R [diagonal, label="Write x"];
        R ->> A [diagonal, label="Written x"];

        A ->> R [diagonal, label="Read"];
        R ->> A [diagonal, label="Read x"];
}
