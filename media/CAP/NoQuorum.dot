graph NoQuorum {
        bgcolor="transparent";
        node [fontname="Helvetica"];
        layout=fdp;
                
        Node1 [label="A"];
        Node2 [label="B"];

        Node3 [label="C"];

        Node4 [label="D"];
        Node5 [label="E"];

        
        {
                edge [penwidth=5, color=blue];
                
                Node1 -- Node2;
        }

        {
                edge [penwidth=5, color=orange];
                
                Node4 -- Node5;
        }
        
      
        {
                edge [color=lightgrey, style=dashed];
                
                Node3 -- Node4;                
                Node3 -- Node5;               
                Node1 -- Node3;
                Node1 -- Node4;
                Node1 -- Node5;
                Node2 -- Node3;
                Node2 -- Node4;
                Node2 -- Node5;
        }
}
